<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.qianfeng">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.activity.DiscoverActivity" />
        <activity android:name="com.example.activity.OrderActivity" />
        <activity android:name="com.example.activity.SearchActivity" />
        <activity android:name="com.example.activity.SearchResultActivity" />
        <activity android:name="com.example.activity.CategoryActivity" />
        <activity android:name="com.example.activity.ProductActivity" />
        <activity android:name="com.example.activity.DiscoverProductActivity" />
        <activity android:name="com.example.activity.CategoryProductActivity" />
        <activity android:name="com.example.activity.SettingsActivity" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name="com.example.activity.HelpActivity"></activity>
    </application>

</manifest>